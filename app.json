{
  "expo": {
    "name": "24Anime",
    "slug": "24Anime",
    "version": "1.0.0",
     "androidStatusBar": {
      "backgroundColor": "#161616",
      "barStyle": "light-content"
    },
    "androidNavigationBar": {
      "backgroundColor": "#161616",
      "barStyle": "light-content",
      "visible": "visible"
    },
 
    "orientation": "portrait",
    "icon": "./assets/images/logo1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo1.png",
        "backgroundColor": "#161616"
      },
      "package": "com.beplop.twentyfouranime"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo1.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#161616"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4d6c8d04-80d6-460a-8f4d-d5fc995b9787"
      }
    }
  }
}
